{
  "name": "semono",
  "version": "0.0.1",
  "description": "A Serial Monitor written with Node.js",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . semono --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Semono\"",
    "package-linux": "electron-packager . semono --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarceloFariaz/Semono.git"
  },
  "keywords": [
    "Serial",
    "Monitor",
    "Node",
    "Node.js",
    "node-serialport",
    "serialport"
  ],
  "author": "MarceloFariaz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarceloFariaz/Semono/issues"
  },
  "homepage": "https://github.com/MarceloFariaz/Semono#readme",
  "devDependencies": {
    "electron": "^1.7.10",
    "serialport": "^6.0.4"
  }
}
